{
  "name": "strapi-generate-admin",
  "version": "1.4.0",
  "description": "Generate the default admin panel for a Strapi application.",
  "homepage": "http://strapi.io",
  "keywords": [
    "generate",
    "generator",
    "strapi"
  ],
  "main": "./lib/index.js",
  "directories": {
    "lib": "./lib"
  },
  "dependencies": {
    "fs-extra": "^0.24.0",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "xo": "^0.11.0"
  },
  "xo": {
    "space": true,
    "esnext": true,
    "envs": [
      "node",
      "mocha"
    ],
    "globals": [
      "strapi",
      "angular",
      "$",
      "_"
    ],
    "ignores": [
      "files/api/admin/public/**",
      "./tmp/**",
      "node_modules/**",
      "bower_components/**",
      "coverage/**",
      "test/**"
    ],
    "rules": {
      "babel/object-shorthand": 0,
      "camelcase": 0,
      "consistent-return": 0,
      "consistent-this": 0,
      "default-case": 0,
      "dot-notation": 0,
      "global-require": 0,
      "max-nested-callbacks": [
        1,
        6
      ],
      "no-else-return": 0,
      "no-extra-parens": 0,
      "no-implicit-coercion": 0,
      "no-invalid-this": 0,
      "no-negated-condition": 0,
      "no-lonely-if": 0,
      "no-throw-literal": 0,
      "no-undef": 0,
      "no-unused-expressions": 0,
      "no-return-assign": 0,
      "padded-blocks": 0,
      "prefer-arrow-callback": 0,
      "prefer-reflect": 0,
      "prefer-template": 0,
      "quote-props": 0,
      "space-before-keywords": 0,
      "strict": 0
    }
  },
  "scripts": {
    "prepublish": "cd ./files/api/admin/public && npm install && gulp dist",
    "test": "xo && cd ./files/api/admin/public && xo"
  },
  "author": {
    "email": "hack@wistity.co",
    "name": "Wistity team",
    "url": "http://wistity.co"
  },
  "maintainers": [
    {
      "name": "Wistity team",
      "email": "hack@wistity.co",
      "url": "http://wistity.co"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/wistityhq/strapi-generate-admin.git"
  },
  "bugs": {
    "url": "https://github.com/wistityhq/strapi/issues"
  },
  "license": "MIT"
}
